{
  "name": "My First Extension",
  "version": "1.0",
  "description": "The first extension that I made.",
  "background_page": "background.html",      
  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "chrome://chromewebdata/"], 
      "js": ["ext.js","content.js"],
      "all_frames": true,
      "run_at":"document_start"
    }
  ],
  
  "permissions": [
    "http://*/",
    "tabs"
  ]
}