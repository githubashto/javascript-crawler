<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="Weather" table="Object_Weather">
		<id name="id" type="long" column="id">
			<generator class="native" />
		</id>
		<property name="locationId" type="string" />
		<property name="castDate" type="date" />
		<property name="condition0" type="string" />
		<property name="temp0" type="integer" />
		<property name="wind0" type="string" />
		<property name="strength0" type="string" />
		<property name="condition1" type="string" />
		<property name="temp1" type="integer" />
		<property name="wind1" type="string" />
		<property name="strength1" type="string" />
		<property name="createTime" type="timestamp" />
		<property name="updateTime" type="timestamp" />
	</class>
</hibernate-mapping>