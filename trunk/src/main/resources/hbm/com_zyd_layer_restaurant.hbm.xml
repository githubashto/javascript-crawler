<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class entity-name="com.zuiyidong.layer.restaurant" table="layer_com_zuiyidong_layer_restaurant">
		<id name="id" type="long" column="id" />
		<property name="lng" type="double" />
		<property name="lat" type="double" />
		<property name="address" type="string" />
		<property name="shopName" type="string" />
		<property name="shopName2" type="string" />
		<property name="city" type="string" />
		<property name="district1" type="string" />
		<property name="district2" type="string" />
		<property name="district3" type="string" />
		<property name="tel" type="string" />
		<property name="tel2" type="string" />
		<property name="areaCode" type="string" />
		<property name="categoryList" type="string" />
		<property name="nearBy" type="string" />
	</class>
</hibernate-mapping>